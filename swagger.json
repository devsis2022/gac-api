{
  "openapi": "3.0.0",
  "info": {
    "title": "gac-api 2022-2",
    "description": "API para acessar os dados de atividades complementares. Trabalho da turma 20211GR.ADS",
    "version": "1.0"
  },
  "servers": [
    {
      "url": "http://localhost:3000",
      "description": "LOCAL"
    }
  ],
  "tags": [
    {
      "name": "Authentication",
      "description": "Rotas destinadas a autenticação"
    },
    {
      "name": "Institutions",
      "description": "Ações referentes a instituições"
    },
    {
      "name": "Courses",
      "description": "Ações referentes aos cursos"
    },
    {
      "name": "Users",
      "description": "Ações e detalhes referentes aos usuários"
    }
  ],
  "paths": {
    "/auth/signin": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Registro de usuário",
        "description": "Registra o usuário no sistema",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "email",
                  "password",
                  "cpf"
                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "henriquefarias.gr015@academico.ifsul.edu.br"
                  },
                  "password": {
                    "type": "string",
                    "example": "gac2022"
                  },
                  "cpf": {
                    "type": "string",
                    "example": "03888889293"
                  },
                  "username": {
                    "type": "string",
                    "example": "henriquefarias"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Registrado com sucesso",
            "content": {
              "application/json": {}
            }
          },
          "422": {
            "description": "Usuário já existe",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "USER_ALREADY_REGISTERED"
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Erro na persistência do usuário",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateEntityFailed"
                }
              }
            }
          }
        }
      }
    },
    "/auth/login": {
      "post": {
        "tags": [
          "Authentication"
        ],
        "summary": "Gerar accessToken",
        "description": "Retorna o token de acesso do usuário",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "email",
                  "password"
                ],
                "properties": {
                  "email": {
                    "type": "string",
                    "example": "henriquefarias.gr015@academico.ifsul.edu.br"
                  },
                  "password": {
                    "type": "string",
                    "example": "gac2022"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Logado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "token": {
                      "type": "string",
                      "example": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpZCI6MSwiaWF0IjoxNjY5NTA5ODk2LCJleHAiOjE2Njk1MTE2OTZ9.JbqP0Rsc2a4c9_I-bh7llrdH1AQleUtYmVuuk6aZH4Y"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Usuário não autorizado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UnauthorizedUser"
                }
              }
            }
          }
        }
      }
    },
    "/institution": {
      "post": {
        "tags": [
          "Institutions"
        ],
        "summary": "Cadastrar nova instituição",
        "description": "O usuário que cadastra a instituição fica vinculada como gestor principal. Instituição cadastrada com status='pendente'",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": [
                  "name"
                ],
                "properties": {
                  "name": {
                    "type": "string",
                    "example": "IFSUL - Instituto Federal de Tecnologia e Ciências sul-riograndense"
                  },
                  "nickname": {
                    "type": "string",
                    "example": "ifsul"
                  },
                  "description": {
                    "type": "string",
                    "example": "Escola técnica e universidade de tecnologia, formando os melhores profissionais de TI do mercado gaúcho"
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "bearer": []
          }
        ],
        "responses": {
          "201": {
            "description": "Cadastrado com sucesso",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateEntity"
                }
              }
            }
          },
          "400": {
            "description": "Requisição não passou na validação",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BadRequest"
                }
              }
            }
          },
          "401": {
            "description": "Usuário não autorizado",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UnauthorizedUser"
                }
              }
            }
          },
          "500": {
            "description": "Este erro acontecerá caso haja algum problema na persistência dos dados. Como o apelido da instituição já existir",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CreateEntityFailed"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Institutions"
        ],
        "summary": "Listar instituições",
        "description": "Retorna uma lista de instituições com base em seu status. O padrão é 'pending'",
        "parameters": [
          {
            "in": "query",
            "name": "status",
            "schema": {
              "type": "string",
              "example": "active | pending | repproved"
            },
            "description": "Filtrar instituições por status. Se este parâmetro não for setado, o padrão é pending"
          },
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "number"
            },
            "description": "Listagem paginada. Página padrão: 1"
          },
          {
            "in": "query",
            "name": "count",
            "schema": {
              "type": "number"
            },
            "description": "Items por página padrão: 10"
          }
        ],
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "page": {
                      "type": "number",
                      "example": 1
                    },
                    "count": {
                      "type": "number",
                      "example": 10
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "id": {
                            "type": "number",
                            "example": 1
                          },
                          "name": {
                            "type": "string",
                            "example": "IFSUL - Instituto Federal de Tecnologia e Ciências sul-riograndense"
                          },
                          "nickname": {
                            "type": "string",
                            "example": "ifsul"
                          },
                          "description": {
                            "type": "string",
                            "example": "Escola técnica e universidade de tecnologia, formando os melhores profissionais de TI do mercado gaúcho"
                          },
                          "status": {
                            "type": "string",
                            "example": "active"
                          },
                          "managerId": {
                            "type": "number",
                            "example": 1
                          },
                          "createdAt": {
                            "type": "string",
                            "example": "2023-02-25T22:35:16.978Z"
                          },
                          "updatedAt": {
                            "type": "string",
                            "example": "2023-02-25T22:35:16.978Z"
                          },
                          "deletedAt": {
                            "type": "string",
                            "example": null
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
